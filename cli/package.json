{
  "name": "jamsocket",
  "version": "0.10.5",
  "description": "A CLI for the Jamsocket platform",
  "author": "Taylor Baldwin <taylor@driftingin.space>",
  "bin": {
    "jamsocket": "./bin/run"
  },
  "homepage": "https://github.com/jamsocket/jamsocket",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "1.9.0",
    "@oclif/plugin-help": "5.1.12",
    "@oclif/plugin-warn-if-update-available": "2.0.4",
    "chalk": "^4",
    "chokidar": "^3.5.3",
    "date-fns": "^2.29.3",
    "eventsource": "^2.0.2",
    "inquirer": "^8.2.5",
    "is-wsl": "^2.2.0",
    "string-length": "4.0.2"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/inquirer": "^8.2.4",
    "@types/node": "^16.9.4",
    "globby": "^11",
    "oclif": "3.0.1",
    "prettier": "^3.3.3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^5.0.0"
  },
  "oclif": {
    "bin": "jamsocket",
    "dirname": "jamsocket",
    "topicSeparator": " ",
    "topics": {
      "service": {
        "description": "manage jamsocket services"
      },
      "backend": {
        "description": "manage jamsocket backends"
      },
      "token": {
        "description": "manage spawn tokens"
      }
    },
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    }
  },
  "scripts": {
    "build": "shx rm -rf lib && tsc -b",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepare": "npm run build",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/jamsocket/jamsocket/issues",
  "keywords": [
    "jamsocket",
    "plane",
    "spawner",
    "session backends",
    "session lived backends",
    "session lived application backends",
    "drifting in space",
    "docker"
  ],
  "types": "lib/index.d.ts"
}
